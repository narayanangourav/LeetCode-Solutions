class Solution:
    def isValid(self, s: str) -> bool:
        c=0
        for i in range(0,len(s),2):
            j=i+1
            
            if (s[i]=="(" and s[j]=="{") or (s[i]=="}" and s[j]==")"):
                continue
                c=0
            elif (s[i]=="(" and s[j]=="[") or (s[i]=="]" and s[j]==")"):
                continue
                c=0
            elif (s[i]=="{" and s[j]=="[") or (s[i]=="]" and s[j]=="}"):
                continue
                c=0
            elif (s[i]=="{" and s[j]=="(") or (s[i]==")" and s[j]=="}"):
                continue
                c=0
            elif (s[i]=="[" and s[j]=="(") or (s[i]==")" and s[j]=="]"):
                continue
                c=0
            elif (s[i]=="[" and s[j]=="{") or (s[i]=="}" and s[j]=="]"):
                continue
                c=0
            elif (s[i]=="(" and s[j]==")") or (s[i]=="{" and s[j]=="}") or (s[i]=="[" and s[j]=="]"):
                continue
                c=0
            else:
                c=1
        if c==0:
	        return (True)
        return (False)
 
